method1 = zeros(1,100);
method2 = zeros(1,100);
method3 = zeros(1,100);

% Run each method k times 
for k = 1 : 100 
    [~,outM1] = system('./method1');
    method1(1,k) = str2double(outM1); 
end 

for k = 1 : 100 
    [~,outM2] = system('./method2');
    method2(k,1) = str2double(outM2); 
end 

for k = 1 : 100
    [~,outM3] = system('./method3');
    method3(k,1) = str2double(outM3); 
end 

% construct graphs 
scatter(method1,1:100)
hold on;
legend
ylabel('Run #');
xlabel('Time milliseconds');

figure
h1 = scatter(method1,Shed,'b','o');
hold on
h2 = scatter(method,Uncertain,'k','.');
hold on
h3 = scatter(Attached_Rg,Attached,'g','x');
legend([h1,h2,h3], 'Shed', 'Uncertain', 'Attached')

%{
pd = makedist('Normal');
pdf_normal = pdf(pd,method1)
plot(method1,pdf_normal,'LineWidth',2)
%}



